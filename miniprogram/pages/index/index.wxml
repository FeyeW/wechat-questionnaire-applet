<cu-custom isBack="{{true}}">
    <view class="cu-bar bg-gradual-blue" style="height:180rpx;padding-top:0rpx !important;">
        <text class="title">个人中心</text>
    </view>
</cu-custom>
<view class="all">
    <view class="content" class="content" wx:for="{{nameList}}" wx:key="index">
        <view class="content-box" data-qid="{{index}}" bindtap="handleUrl">
            <image mode="widthFix" src="https://s1.ax1x.com/2022/03/24/qG8klR.png" />
        </view>
        <view class="content-ashin" data-id="{{index}}" bindtap="handleDelete">
            <image mode="widthFix" src="https://s1.ax1x.com/2022/03/25/qtpL24.png" />
        </view>
        <text>{{item}}</text>
    </view>
</view>